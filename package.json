{
  "name": "express-locallibrary-tutorial",
  "version": "0.0.0",
  "engines": {
    "node": "10.15.1"
  },
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "devstart": "nodemon ./bin/www",
    "debugstart": "nodemon --inspect=0.0.0.0 ./bin/www",
    "seed": "node ./populatedb.js $MONGODB_URI",
    "test": "jest --reporters='default' --reporters='jest-junit'"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "coverageReporters": [
      "text-summary",
      "html"
    ],
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverage": true,
    "testMatch": [
      "**/*.spec.js"
    ]
  },
  "dependencies": {
    "async": "^2.6.2",
    "compression": "^1.7.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-validator": "^5.3.1",
    "helmet": "^3.15.1",
    "http-errors": "~1.6.2",
    "moment": "^2.24.0",
    "mongoose": "^5.4.13",
    "morgan": "~1.9.0",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "jest": "^24.8.0",
    "nodemon": "^1.18.10",
    "request": "^2.88.0",
    "supertest": "^4.0.2",
    "jest-junit": "^7.0.0"
  }
}
